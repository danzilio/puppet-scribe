{
  "name": "danzilio-scribe_reporter",
  "version": "1.0.0",
  "author": "David Danzilio",
  "summary": "A report processor that sends whole Puppet reports to a Scribe log server.",
  "license": "Apache-2.0",
  "source": "https://github.com/danzilio/puppet-scribe_reporter",
  "project_page": "https://github.com/danzilio/puppet-scribe_reporter",
  "issues_url": "https://github.com/danzilio/puppet-scribe_reporter/issues",
  "tags": [
    "scribe",
    "reports",
    "logs",
    "reporting"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 2.7.26"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=2.6.0 <5.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 1.1.3 <2.0.0"
    },
    {
      "name": "danzilio/report_all_the_things",
      "version_requirement": ">=1.0.0 <2.0.0"
    },
  ]
}
